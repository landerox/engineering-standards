  "ignoreWords": [
    "nd",
    "cte",
    "sav",
    "ser"
  ],
  "import": [],
  "dictionaries": [
    "en_US",
    "softwareTerms",
    "typescript",
    "python",
    "devops"
  ],
  "dictionaryDefinitions": [],
  "useGitignore": true,
  "ignorePaths": [
    ".git",
    ".venv",
    "__pycache__",
    "*.egg-info",
    "dist",
    "build",
    ".pytest_cache",
    ".coverage",
    "htmlcov",
    "node_modules",
    ".pre-commit-cache",
    "uv.lock"
  ],
  "overrides": [
    {
      "filename": "**/*.md",
      "ignoreRegExpList": [
        "\\[.*\\]\\(.*\\)",
        "^#+\\s",
        "```[\\s\\S]*?```"
      ]
    },
    {
      "filename": "**/*.yaml",
      "language": "yaml"
    },
    {
      "filename": "**/*.json",
      "language": "json"
    }
  ],
  "noSuggestDictionaries": [
    "fonts",
    "filetypes",
    "software-terms"
  ],
  "allowCompoundWords": true,
  "caseSensitive": false
}
